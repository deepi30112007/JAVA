import java.util.Scanner;
public class panlindrome{
  public static void main(String args[]){
    Scanner sc=new Scanner(System.in);
    int n=121;
    temp=n;
    int rev=0;
    while(n!=0){
      rev=rev*10+n%10;
      n/=10;
    }
    System.out.println(temp==rev?"palindrome":"not palindrome");
  }
}