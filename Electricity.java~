import java.util.Scanner;
public class Electricity{
  public static void main(String args[]){
    Scanner sc=new Scanner(System.in);
    int units=sc.nextInt();
    int bill;
    if(units<=100){
      bill=units*1;
    }
    else if{units<=200){
      bill=100*1+(units-100)*2;
    }
    System.out.println("billamount='"rupee"+bill);
                         
                         }
}
    